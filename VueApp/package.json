{
    "name": "vueapp",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "20.19.4"
    },
    "scripts": {
        "dev": "vite",
        "dev:typed": "concurrently --names \"VITE,TYPES\" --prefix-colors \"green,blue\" \"vite\" \"vue-tsc --noEmit --watch\"",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "type-check:watch": "vue-tsc --noEmit --watch",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "watch": "node vite-watch.js",
        "build-dev": "run-p type-check \"build-only-dev {@}\" --",
        "build-only-dev": "vite build --mode development",
        "build-test": "run-p type-check \"build-only-test {@}\" --",
        "build-only-test": "vite build --mode test",
        "test": "vitest",
        "test:run": "vitest run"
    },
    "dependencies": {
        "@quasar/extras": "^1.16.12",
        "@vueuse/core": "^13.8.0",
        "chart.js": "^4.4.3",
        "glob": "^11.0.3",
        "pinia": "^3.0.3",
        "quasar": "^2.18.2",
        "vue": "^3.4.21",
        "vue-chartjs": "^5.3.1",
        "vue-router": "^4.3.3",
        "vuedraggable": "^4.1.0"
    },
    "devDependencies": {
        "@pinia/testing": "^1.0.2",
        "@quasar/app-vite": "^2.4.0",
        "@quasar/vite-plugin": "^1.9.0",
        "@tsconfig/node20": "^20.1.4",
        "@types/node": "~20.19.0",
        "@typescript-eslint/eslint-plugin": "^8.43.0",
        "@typescript-eslint/parser": "^8.41.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.8.1",
        "chokidar": "^4.0.3",
        "concurrently": "^9.2.1",
        "eslint": "^9.34.0",
        "eslint-plugin-quasar": "^1.1.0",
        "eslint-plugin-vue": "^10.4.0",
        "eslint-plugin-vuejs-accessibility": "^2.4.1",
        "globals": "^16.3.0",
        "happy-dom": "^18.0.1",
        "lodash-es": "^4.17.21",
        "npm-run-all2": "^8.0.4",
        "sass": "^1.91.0",
        "tree-kill": "^1.2.2",
        "typescript": "^5.9.2",
        "vite": "^7.1.5",
        "vite-plugin-vue-inspector": "^5.3.2",
        "vitest": "^3.2.4",
        "vue-tsc": "^3.0.6"
    },
    "volta": {
        "node": "20.19.4"
    }
}
