{
  "name": "viper2",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev:backend": "cd web && dotnet watch run --launch-profile https",
    "dev:frontend": "cd VueApp && npm install && npm run dev",
    "dev:frontend-build": "cd VueApp && npm install && npm run build",
    "lint": "node scripts/lint-any.js",
    "lint:precommit": "lint-staged",
    "lint:staged": "cross-env LINT_BLOCK_ON_WARNINGS=true lint-staged",
    "prepare": "husky",
    "test:backend": "cd test && dotnet test",
    "test:frontend": "cd VueApp && npm run test:run"
  },
  "devDependencies": {
    "@double-great/stylelint-a11y": "^3.0.4",
    "cross-env": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript-eslint": "^8.40.0"
  },
  "volta": {
    "node": "20.6.1"
  }
}
